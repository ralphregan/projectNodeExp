<link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css" rel="stylesheet"
    integrity="sha384-QWTKZyjpPEjISv5WaRU9OFeRpok6YctnYmDr5pNlyT2bRjXh0JMhjY6hW+ALEwIH" crossorigin="anonymous">
    
    <link rel="stylesheet" href="/styles/main.css">
    
    <%- include("partials/header.ejs") %>
<div class="container">
    
        <div class="d-flex gap-3 mt-4"><h2>Start</h2><a href="/start" class="btn btn-success">YourJourney</a> </div>
            
    <% if(locals.articles){ %>
        <% console.log(articles.title); %>
        
        <% articles.forEach(article=> { %>
            <div class="card mt-4">
                <div class="card-body d-flex justify-content-between">
        
                    <div >
                        <div class="card-title lead">
                            <h1>
                                <%= article.title %>
                            </h1>
                        </div>
    
                        <div class="card-subtitle text-muted mt-4">
                            <%= article.CreatedTime.toLocaleDateString() %>
                        </div>
                        <div class="card-text mt-4">
                            <%= article.description %>
                        </div>
                        <a href="home/<%=article.slug %>" class="btn btn-primary mt-3">Read More</a>
                    
                    </div>
                    <div class="">
                        <img id="pub_img" src="<%= article.imageUrl %>" alt="Article Image">
                         </div>
                    

                </div>

            </div>

            <% }); %>
        
        
       
        <% } %>
</div>