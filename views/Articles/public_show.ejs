
<link rel="preconnect" href="https://fonts.googleapis.com">
<link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>

<link href="https://fonts.googleapis.com/css2?family=Sorts+Mill+Goudy:ital@0;1&display=swap" rel="stylesheet">

<link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css" rel="stylesheet"
    integrity="sha384-QWTKZyjpPEjISv5WaRU9OFeRpok6YctnYmDr5pNlyT2bRjXh0JMhjY6hW+ALEwIH" crossorigin="anonymous">
 <%- include("../partials/header.ejs") %>
    <link rel="stylesheet" href="/styles/public.css">
<link rel="stylesheet" href="/styles/main.css">
  
    
<div id="pub_container" class="container">
    <h1 class="mb-1 pub-h1">
         <strong> <%= article.title %></strong>
    </h1>
    <div class="mb-2 ml-5 d-inline"><h6 class="pub-h1">Filed by <%= article.arthur %> on <u><%= article.CreatedTime.toLocaleDateString() %></u></h6> </div>
   
    
    
    <div id="pic" class="d-flex gap-5 justify-content-around text-center " >
        <img src="<%= article.imageUrl%>" alt="Article Image" height="350px" width="500px">
        <div id="desc"> <%= article.description%></div>
    </div>

    <div  id="thePath">
        <%= article.path %>
    </div>
    <a href="/home" class="btn btn-primary mt-3">Home</a>

    

</div>